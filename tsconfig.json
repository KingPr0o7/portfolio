{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@src/*": [ "src/*" ],
			"@public/*": [ "public/*" ],
			"@images/*": [ "src/images/*" ],
			"@branding/*": [ "src/images/branding/*" ],
			"@personal/*": [ "src/images/personal/*" ],
			"@people/*": [ "src/images/people/*" ],

			"@icons/*": [ "src/images/icons/*" ],
			"@logos/*": [ "src/images/icons/logos/*" ],
			"@symbols/*": [ "src/images/icons/symbols/*" ],

			"@layouts/*": [ "src/layouts/*" ],
			"@pages/*": [ "src/pages/*" ],
			"@components/*": [ "src/components/*" ],
		},
	"jsx": "react-jsx",
	"jsxImportSource": "react"
	}
}